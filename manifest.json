{
    "name": "Vaynerblock",
    "version": "1.0",
    "description": "Block content that contains our friend Gary",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/faceblock_16.png",
            "32": "images/faceblock_32.png",
            "48": "images/faceblock_48.png",
            "128": "images/faceblock_128.png"
        }
    },
    "content_scripts": [{
        "js": ["content.js"],
        "matches": ["https://*.linkedin.com/*", "https://linkedin.com/*"],
        "run_at": "document_end"
    }],
    "icons": {
        "16": "images/faceblock_16.png",
        "32": "images/faceblock_32.png",
        "48": "images/faceblock_48.png",
        "128": "images/faceblock_128.png"
    }
}
